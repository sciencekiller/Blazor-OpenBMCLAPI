@page "/register"
@inject IStringLocalizer<Resources.Auth> i18n
@inject NavigationManager NavigationManager
<h1 style="font-size:20px">@i18n["register_title"]</h1>
<p>@i18n["register_intro"]</p>
<Blazor_OpenBMCLAPI.Components.UserInfoInput inputType="register"/>
@code {
    protected override async Task OnInitializedAsync()
    {
        if (await db.IsUserExist()) NavigationManager.NavigateTo("/unauth",true);
    }
}
